q=input()
for i in range(0,q):
    s=raw_input()
    s=s.split()
    s=map(float,s)
    T1=s[0]
    T2=s[1]
    t1=s[2]
    t2=s[3]
    p1=T2*(1-t1/T1)
    p1=p1*(T1-t1)
    p1=p1/2
    p2=T1*(1-t2/T2)
    p2=p2*(T2-t2)
    p2=p2/2
    p3=T1*T2
    p1=p1/p3
    p2=p2/p3
    p=1-p1-p2
    print ("%.6f" %p)
    
