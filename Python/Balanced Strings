t=input()
for j in range(0,t):
    x=raw_input()
    f=0
    a=[]
    for i in range (0,26):
        a.append(0)
    for i in range (0,len(x)):
        a[ord(x[i])-97]=a[ord(x[i])-97]+1
    for i in range (0,26):
        if a[i]==0:
            f=1
    if f==1:
        print "No"
    else:
        print "Yes"
